<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.Button?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.petcarecab302qu.controller.DietController"
            style="-fx-background-color: #f0f0f0;">

    <VBox alignment="TOP_LEFT" spacing="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
          style="-fx-background-color: white; -fx-padding: 20px; -fx-border-radius: 5px; -fx-background-radius: 5px; -fx-border-color: #dcdcdc; -fx-border-width: 1px;">


        <Text text="Working" style="-fx-font-size: 24px; -fx-font-weight: bold;" />
        <Button text="Back" onAction="#handleBackButton" style="-fx-font-size: 14px; -fx-background-color: #ff6347; -fx-text-fill: white;"/>

    </VBox>
</AnchorPane>
